{
  "name": "villa-property-management",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.18.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev --port 3000",
    "dev:turbo": "next dev --turbopack --port 3000",
    "build": "next build",
    "build:production": "NODE_ENV=production next build",
    "build:static": "next build && next export",
    "build:analyze": "ANALYZE=true npm run build",
    "build:clean": "npm run clean:production && npm run build",
    "build:stats": "npm run build:analyze && npx webpack-bundle-analyzer .next/analyze/bundle/client.html",
    "deps:check": "npx depcheck",
    "deps:audit": "npm audit --audit-level moderate",
    "deps:update": "npx npm-check-updates -u",
    "hygiene:check": "node scripts/pre-commit-hygiene.js",
    "hygiene:clean": "node scripts/clean.js all",
    "analyze": "node scripts/analyze-bundle.js analyze",
    "analyze:deps": "node scripts/analyze-bundle.js deps",
    "analyze:report": "node scripts/analyze-bundle.js report",
    "start": "next start",
    "start:production": "NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "npm run clean:all",
    "clean:all": "npm run clean:build && npm run clean:cache && npm run clean:deps",
    "clean:build": "rm -rf .next out dist build .vercel .turbo",
    "clean:cache": "rm -rf node_modules/.cache .eslintcache .tsbuildinfo .swc",
    "clean:deps": "rm -rf node_modules package-lock.json yarn.lock pnpm-lock.yaml",
    "clean:install": "npm run clean:all && npm install",
    "clean:production": "npm run clean:build && npm run clean:cache",
    "setup:database": "node scripts/setup-database.js",
    "create:notifications": "node scripts/create-notification-tables.js",
    "create:notifications-table": "node scripts/create-notifications-table.js",
    "dev:debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "firebase:start": "firebase emulators:start",
    "firebase:deploy": "firebase deploy",
    "firebase:login": "firebase login",
    "firebase:logout": "firebase logout",
    "staff:list": "node scripts/staff-fix.js list",
    "staff:fix": "node scripts/staff-fix.js fix-all",
    "staff:fix-one": "node scripts/staff-fix.js fix",
    "mobile:verify": "node -e \"const TestJobService = require('./src/services/TestJobService').default; TestJobService.verifyStaffUserIds().then(console.log)\"",
    "mobile:test-job": "node -e \"const TestJobService = require('./src/services/TestJobService').default; TestJobService.createTestJob().then(console.log)\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.18",
    "@fullcalendar/daygrid": "^6.1.18",
    "@fullcalendar/interaction": "^6.1.18",
    "@fullcalendar/list": "^6.1.18",
    "@fullcalendar/react": "^6.1.18",
    "@fullcalendar/resource-daygrid": "^6.1.18",
    "@fullcalendar/resource-timegrid": "^6.1.18",
    "@fullcalendar/timegrid": "^6.1.18",
    "@googlemaps/js-api-loader": "^1.16.10",
    "@googlemaps/react-wrapper": "^1.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@turf/distance": "^7.2.0",
    "@turf/helpers": "^7.2.0",
    "@turf/turf": "^7.2.0",
    "@types/nodemailer": "^6.4.17",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "firebase": "^11.10.0",
    "firebase-admin": "^13.4.0",
    "framer-motion": "^12.23.0",
    "lenis": "^1.3.4",
    "lucide-react": "^0.525.0",
    "mongodb": "^6.17.0",
    "next": "15.3.4",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.4",
    "openai": "^5.9.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.60.0",
    "react-hot-toast": "^2.5.2",
    "recharts": "^3.0.2",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.75"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.4.2",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/mongodb": "^4.0.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.0",
    "typescript": "^5"
  }
}
