{
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },

  "functions": {
    "source": "functions",
    "runtime": "nodejs20"
  },

  "storage": {
    "rules": "storage.rules"
  },

